AAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,WAAA,CAAA,UAAA,CAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,kBAAA,KAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA;AACA,iBAHA,EAIA,KAJA,CAIA,UAAA,GAAA,EAAA;AACA,kBAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,GAAA;;AACA,kBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,oBAAA,IAAA,EAAA,MADA;AAEA,oBAAA,MAAA,EAAA;AAAA,sBAAA,KAAA,EAAA;AAAA;AAFA,mBAAA;AAIA,iBAVA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KArBA;AAuBA,IAAA,UAvBA,sBAuBA,KAvBA,EAuBA;AACA,WAAA,OAAA,GAAA,KAAA;AACA,KAzBA;AA2BA,IAAA,WA3BA,uBA2BA,KA3BA,EA2BA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,MADA;AAEA,QAAA,MAAA,EAAA;AAAA,UAAA,OAAA,EAAA;AAAA;AAFA,OAAA;AAIA;AAhCA,GAhBA;AAmDA,EAAA,WAnDA,yBAmDA;AACA,QAAA,CAAA,KAAA,sBAAA,EAAA,EAAA;AACA,WAAA,UAAA;AACA;AACA;AAvDA,CAAA","sourcesContent":["<template>\n  <div>\n    <div>\n      <!-- src=\"https://baazarproductions.com/wp-content/uploads/2018/02/background-horizontal-dark-wood.jpg\" -->\n      <!-- src=\"https://cdn.vuetifyjs.com/images/backgrounds/vbanner.jpg\" -->\n      <v-container id=\"profile-header\">\n        <v-row\n          align=\"center\"\n          justify=\"center\"\n        >\n          <v-col class=\"text-center\" cols=\"12\">\n            <v-avatar size=\"150\" class=\"mb-5\">\n              <v-img\n                :src=\"profile.image_url\"\n                lazy-src=\"https://via.placeholder.com/50.png\"\n              />\n            </v-avatar>\n\n            <h1 class=\"display-1  mb-4\">\n              {{ profile.name }}\n            </h1>\n\n            <h3 class=\" mb-3\">\n              @{{ profile.username }}\n            </h3>\n\n            <div v-if=\"profile.organizations.length > 0\" class=\"mt-5\">\n              <h4 class=\"subheading\">\n                Organizations\n              </h4>\n\n              <div>\n                <v-btn\n                  dark\n                  dense\n                  depressed\n                  class=\"ma-3\"\n                  color=\"blue-grey\"\n                  v-for=\"org in profile.organizations\"\n                  :key=\"org.id\"\n                >\n                  {{ org }}\n                </v-btn>\n              </div>\n\n            </div>\n\n            <p class=\"text-h6 mt-4 mb-1\">\n              Profile actions\n            </p>\n            <v-row dense class=\"\">\n              <v-spacer></v-spacer>\n              <v-col cols=\"1\">\n                <v-btn\n                  dense\n                  icon\n                  class=\"mx-2\"\n                  color=\"blue-grey\"\n                  :href=\"profile.url\"\n                >\n                  <v-icon small>fab fa-github</v-icon>\n                </v-btn>\n              </v-col>\n\n              <v-col cols=\"1\">\n                <dialog-form :profile=\"profile\" @updatedEvent=\"treatEvent\" refresh/>\n              </v-col>\n\n              <v-col cols=\"1\">\n                <dialog-form :profile=\"profile\" @updatedEvent=\"treatEvent\"/>\n              </v-col>\n\n              <v-col cols=\"1\">\n                <dialog-delete :profile=profile @deletedEvent=\"treatDelete\"/>\n              </v-col>\n              <v-spacer></v-spacer>\n            </v-row>\n          </v-col>\n        </v-row>\n      </v-container>\n    </div>\n\n    <div class=\"mt-3 grey lighten-3\">\n      <v-container class=\"pt-10 pb-10\">\n        <p class=\"display-1 text-center mt-5 mb-10\">\n          Profile data from github\n        </p>\n\n        <v-row>\n          <v-col\n            sm=\"12\"\n            md=\"8\" offset-md=\"2\"\n          >\n            <v-simple-table id=\"information-table\">\n              <template v-slot:default>\n                <tbody>\n                  <tr>\n                    <td class=\"font-weight-bold\">Full name</td>\n                    <td>{{ profile.name }}</td>\n                  </tr>\n                  <tr>\n                    <td class=\"font-weight-bold\">Github username</td>\n                    <td>{{ profile.username }}</td>\n                  </tr>\n                  <tr>\n                    <td class=\"font-weight-bold\">Email</td>\n                    <td>Email is only shown on Github for logged-in users.</td>\n                  </tr>\n                  <tr>\n                    <td class=\"font-weight-bold\">Stars</td>\n                    <td>{{ profile.stars }}</td>\n                  </tr>\n                  <tr>\n                    <td class=\"font-weight-bold\">Followers</td>\n                    <td>{{ profile.followers }}</td>\n                  </tr>\n                  <tr>\n                    <td class=\"font-weight-bold\">Following</td>\n                    <td>{{ profile.subscriptions }}</td>\n                  </tr>\n                  <tr>\n                    <td class=\"font-weight-bold\">Contributions in last year</td>\n                    <td>{{ profile.contributions }}</td>\n                  </tr>\n                </tbody>\n              </template>\n            </v-simple-table>\n          </v-col>\n        </v-row>\n      </v-container>\n    </div>\n\n  </div>\n</template>\n\n<script>\nimport Factory from '@/api/factory'\n\nimport DialogForm from '@/components/DialogForm'\nimport DialogDelete from '@/components/DialogDelete'\n\nconst ProfilesAPI = Factory.getApi('profiles')\n\nexport default {\n  name: 'ProfileView',\n\n  components: {\n    DialogForm,\n    DialogDelete\n  },\n\n  data () {\n    return {\n      profile: {},\n      formattedProfileInfo: [],\n      errors: []\n    }\n  },\n\n  methods: {\n    checkRouteGivenProfile () {\n      if (this.$route.params.profile === undefined) {\n        return false\n      }\n      this.profile = this.$route.params.profile\n      return true\n    },\n\n    async getProfile () {\n      await ProfilesAPI.getProfile(this.$route.params.id)\n        .then((res) => {\n          this.profile = res.data\n        })\n        .catch((err) => {\n          this.errors.push(err)\n          this.$router.push({\n            name: 'home',\n            params: { error: err }\n          })\n        })\n    },\n\n    treatEvent (value) {\n      this.profile = value\n    },\n\n    treatDelete (value) {\n      this.$router.push({\n        name: 'home',\n        params: { message: 'Profile deleted successfully!' }\n      })\n    }\n  },\n\n  beforeMount () {\n    if (!this.checkRouteGivenProfile()) {\n      this.getProfile()\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n\n</style>\n"],"sourceRoot":"src/views"}]}