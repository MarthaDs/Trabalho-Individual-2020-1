A,CAAA,cAAA,EAAA,cAAA;;AACA,kBAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,kBAAA,KAAA,CAAA,MAAA,GAAA,KAAA;AACA,iBAPA,EAQA,KARA,CAQA,UAAA,GAAA,EAAA;AACA,kBAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,GAAA;;AACA,kBAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,iBAXA,CANA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkBA,KAnBA;AAqBA,IAAA,SArBA,uBAqBA;AACA,WAAA,MAAA,GAAA,IAAA;AACA,KAvBA;AAyBA,IAAA,UAzBA,wBAyBA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,KA3BA;AA6BA,IAAA,MA7BA,oBA6BA;AACA,WAAA,OAAA,GAAA,IAAA;AACA,WAAA,aAAA;AACA;AAhCA;AAbA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-fab-transition>\n      <v-btn\n        id=\"modal-open\"\n        color=\"blue-grey\"\n        dark\n        fixed\n        bottom\n        right\n        fab\n        @click.stop=\"showModal\"\n      >\n        <v-icon small>fas fa-user-plus</v-icon>\n      </v-btn>\n    </v-fab-transition>\n\n    <v-dialog v-model=\"dialog\" max-width=\"600px\">\n      <v-card>\n        <v-card-title>\n          <span class=\"headline\">\n            Add new profile\n          </span>\n        </v-card-title>\n\n        <v-card-text>\n          <v-container>\n            <div\n              v-if=\"errors.length > 0\"\n            >\n              <v-alert\n                v-for=\"err in errors\"\n                :key=\"err.id\"\n                dense\n                dismissible\n                type=\"error\"\n              >\n                {{ err }}\n              </v-alert>\n            </div>\n\n            <v-row>\n              <v-col cols=\"12\">\n                <v-text-field\n                  dense\n                  outlined\n                  required\n                  label=\"Name\"\n                  hint=\"Ex.: Bruce Wayne\"\n                  placeholder=\"Ex.: Bruce Wayne\"\n                  v-model=\"name\"\n                />\n              </v-col>\n              <v-col cols=\"12\">\n                <v-text-field\n                  dense\n                  outlined\n                  required\n                  label=\"Github URL\"\n                  hint=\"Ex.: https://github.com/totally-not-batman\"\n                  placeholder='totally-not-batman'\n                  prefix='https://github.com/'\n                  v-model=\"url\"\n                />\n              </v-col>\n            </v-row>\n          </v-container>\n        </v-card-text>\n        <v-card-actions class=\"pb-6 px-6\">\n          <v-spacer></v-spacer>\n          <v-btn\n            id=\"modal-close\"\n            text\n            color=\"blue-grey\"\n            @click=\"closeModal\"\n          >\n            Close\n          </v-btn>\n          <v-btn\n            id=\"modal-create\"\n            depressed\n            dark\n            color=\"blue-grey\"\n            :loading=\"loading\"\n            @click=\"create\"\n          >\n            <v-icon small left>fas fa-user-plus</v-icon> Create\n          </v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </div>\n</template>\n\n<script>\nimport Factory from '@/api/factory'\n\nconst ProfileAPI = Factory.getApi('profiles')\n\nexport default {\n  name: 'AddProfileFab',\n\n  data () {\n    return {\n      name: '',\n      url: '',\n      dialog: false,\n      loading: false,\n      errors: []\n    }\n  },\n\n  methods: {\n    async createProfile () {\n      const profileData = {\n        name: this.name,\n        url: `https://github.com/${this.url}`\n      }\n\n      await ProfileAPI.createProfile(profileData)\n        .then((res) => {\n          const createdProfile = res.data\n          // pass created profile info to parent\n          this.$emit('createdEvent', createdProfile)\n          this.loading = false\n          this.dialog = false\n        })\n        .catch((err) => {\n          this.errors.push(err)\n          this.loading = false\n        })\n    },\n\n    showModal () {\n      this.dialog = true\n    },\n\n    closeModal () {\n      this.dialog = false\n    },\n\n    create () {\n      this.loading = true\n      this.createProfile()\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/components"}]}