,iBAHA,EAIA,KAJA,CAIA,UAAA,GAAA,EAAA;AACA,kBAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,GAAA;AACA,iBANA,CAJA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA;AAZA,GAjBA;AAgCA,EAAA,WAhCA,yBAgCA;AACA,QAAA,KAAA,MAAA,CAAA,MAAA,CAAA,KAAA,KAAA,SAAA,EAAA;AACA,UAAA,KAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,OAAA,KAAA,qCAAA,EAAA;AACA,aAAA,WAAA,GAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAAA;AACA,OAFA,MAEA;AACA,aAAA,WAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,KAAA;AACA;AACA;;AAEA,QAAA,KAAA,MAAA,CAAA,MAAA,CAAA,OAAA,KAAA,SAAA,EAAA;AACA,WAAA,OAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,OAAA;AACA;AACA;AA5CA,CAAA","sourcesContent":["<template>\n\n  <div id=\"home\">\n    <div class=\"mb-5\">\n\n      <v-container fluid>\n\n        <v-alert\n          id=\"alert-error\"\n          v-if=\"legacyError !== undefined\"\n          dense\n          dismissible\n          type=\"error\"\n        >\n          {{ legacyError.message }}\n        </v-alert>\n\n        <v-alert\n          id=\"alert-message\"\n          v-if=\"message !== undefined\"\n          dense\n          dismissible\n          type=\"success\"\n        >\n          {{ message }}\n        </v-alert>\n\n        <div class=\"text-center mt-10 mb-5 py-2 grey--text text--darken-3\">\n          <span class=\"text-h1\">\n            <v-icon large class=\"grey--text text--darken-3\">fab fa-github</v-icon>\n          </span>\n          <h1>\n            Github profile indexer\n          </h1>\n          <h2 class=\"text-h4 text-subtitle-1\">\n            Find your favorite Github profiles here\n          </h2>\n        </div>\n\n        <v-row>\n          <v-col cols=\"12\" sm='8' offset-sm='2' md='6' offset-md='3'>\n            <v-form @submit=\"makeSearch()\">\n              <v-row no-gutters>\n                <v-col cols=\"12\" sm='10'>\n                  <v-text-field\n                    outlined\n                    dense\n                    :error-messages=\"errors.length > 0 ? 'Some errors occured with your request!' : null\"\n                    class=\"rounded-l rounded-r-0\"\n                    placeholder='Search by name, username, location, etc.'\n                    v-model=\"searchString\"\n                  />\n                </v-col>\n                <v-col cols=\"12\" sm='2'>\n                  <v-btn\n                    id=\"home-make-search\"\n                    color='primary'\n                    class=\"py-5 rounded-r rounded-l-0\"\n                    :disabled=\"!searchString\"\n                    @click=\"makeSearch()\"\n                  >\n                    Search\n                  </v-btn>\n                </v-col>\n              </v-row>\n            </v-form>\n          </v-col>\n        </v-row>\n\n      </v-container>\n    </div>\n\n    <div class=\"grey lighten-3 py-md-8 py-sm-4\">\n      <v-container>\n        <div\n          class=\"text-h4 grey--text lighten-1 text-center py-md-10 py-sm-3\"\n          v-if=\"searchResults === undefined\"\n        >\n          Use the search bar to search for users\n        </div>\n\n        <div\n          class=\"text-h4 grey--text lighten-1 text-center py-md-10 py-sm-3\"\n          v-else-if=\"searchResults.length === 0\"\n        >\n          No results found\n        </div>\n\n        <results-list v-else\n          :search=\"searchString\"\n          :results=\"searchResults\"\n          @updatedProfile=\"makeSearch()\"\n        />\n      </v-container>\n    </div>\n\n  </div>\n\n</template>\n\n<script>\nimport ResultsList from '@/components/ResultsList'\nimport Factory from '@/api/factory'\n\nconst SearchAPI = Factory.getApi('search')\n\nexport default {\n  name: 'Home',\n\n  data () {\n    return {\n      searchString: undefined,\n      searchResults: undefined,\n      legacyError: undefined,\n      message: undefined,\n      errors: []\n    }\n  },\n\n  components: {\n    ResultsList\n  },\n\n  methods: {\n    async makeSearch (value) {\n      // redo search on update profile\n      const searchQuery = value !== undefined ? value : this.searchString\n\n      await SearchAPI.searchProfiles(searchQuery)\n        .then((res) => {\n          this.searchResults = res.data\n        })\n        .catch((err) => {\n          this.errors.push(err)\n        })\n    }\n  },\n\n  beforeMount () {\n    if (this.$route.params.error !== undefined) {\n      if (this.$route.params.error.message === 'Request failed with status code 404') {\n        this.legacyError = { message: 'Profile not found!' }\n      } else {\n        this.legacyError = this.$route.params.error\n      }\n    }\n\n    if (this.$route.params.message !== undefined) {\n      this.message = this.$route.params.message\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views"}]}